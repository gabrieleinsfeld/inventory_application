<form action="/add/youtuber" method="POST">
  <label for="youtuber_name">Youtuber Name: </label>
  <input type="text" name="youtuber_name" />
  <label for="youtuber_channel">Youtuber Channel: </label>
  <input type="text" name="youtuber_channel" />
